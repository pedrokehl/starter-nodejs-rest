{
    "extends": "airbnb",
    "rules": {
        "brace-style": ["error", "stroustrup"],
        "comma-dangle": "off",
        "indent": ["error", 4],
        "linebreak-style": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "no-unused-vars": ["error", { "args": "none" }]
    }
}